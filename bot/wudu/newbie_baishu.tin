#function {_trim} {#format result {%p} {%1}};

#alias {_baishu} {
    #math i 1;
    #while {&book_colors[+$i]} {
        #NOP #showme {$book_colors[+$i] -> @_trim{$book_names[+$i]}};
        #math j $i + 1;
        #local found 0;
        #while {&book_colors[+$j]} {
            #if {$book_colors[+$j] < $book_colors[+$i]} {
                #NOP #showme {$book_colors[+$j] - @_trim{$book_names[+$j]}};
                #showme {<efa>zhengli @_trim{$book_names[+$j]} to $i};
                #delay {2} {zhengli @_trim{$book_names[+$j]} to $i};
                #class newbie_baishu kill;
                #class newbie_baishu read bot/wudu/newbie_baishu.tin;
                #math found 1;
                #break;
            };
            #math j $j + 1;
        };
        #if {$found == 1} {
            #break;
        }
        #math i $i + 1;
    };
};

