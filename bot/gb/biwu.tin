#class biwu open;

#alias {biwu.start} {
    #show {<efa>开始准备比武...} ;
    #action {任务】比武大会主持人: %*夺取血光旗！} {gb};
    #action {^由于你为师门在江湖上的威望提高做出了贡献，你获得了} {gb;bow};
    #action {^【任务】比武大会主持人: 比武大会第%1轮结束！$} {gb;bow};
    #action {^【任务】比武大会主持人: 下一轮%1以下，一分钟后开始！} {gb;bow};
    #action {^【任务】比武大会主持人: 比武大会第%1轮，现在开始} {enter;duo};
};

#alias {biwu.stop} {
    #show {<efa>结束比武...};
    #class biwu kill;
};

#class biwu close;
