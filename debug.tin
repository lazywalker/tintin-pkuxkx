#alias {debugon} {
    #config {VERBOSE} {ON};
    #DEBUG {LIST} {ON};
    #CONFIG {VERBOSE} {ON};
    #DEBUG {ALL} {ON};
    #DEBUG;

    #log APPEND {log/debug.log};
    #debug {ALL} {LOG};
};

#alias {debugoff} {
    #config {VERBOSE} {OFF};
    #DEBUG {LIST} {OFF};
    #CONFIG {VERBOSE} {OFF};
    #DEBUG {ALL} {OFF};
    #DEBUG;
};

#alias {printon} {
    #NOP use #unalias {%*} to stop print;
    #alias {%*} { #showme {<fab>MSG:INPUT       %0}; };
};
